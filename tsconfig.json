{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "target": "es6",
    "jsx": "react"
  },
  "include": ["./**/*.ts", "./**/*.tsx", "./**/*.json"]
}
